{
  "name": "@gilbarbara/react-range-slider",
  "version": "0.5.0",
  "description": "A range slider component for React",
  "author": "Gil Barbara <gilbarbara@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/gilbarbara/react-range-slider.git"
  },
  "bugs": {
    "url": "https://github.com/gilbarbara/react-range-slider/issues"
  },
  "homepage": "https://github.com/gilbarbara/react-range-slider",
  "keywords": [
    "react",
    "react-component",
    "range",
    "slider"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "files": [
    "esm",
    "lib"
  ],
  "types": "esm",
  "sideEffects": false,
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "deepmerge": "^4.2.2"
  },
  "devDependencies": {
    "@gilbarbara/tsconfig": "^0.1.0",
    "@size-limit/preset-small-lib": "^4.0.2",
    "@types/blacklist": "^1.1.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@types/once": "^1.4.0",
    "@types/react": "^16.9.21",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "cross-env": "^7.0.0",
    "del-cli": "^3.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "husky": "^4.2.3",
    "is-ci-cli": "^2.0.0",
    "jest": "^25.1.0",
    "jest-enzyme": "^7.1.2",
    "jest-extended": "^0.11.5",
    "jest-serializer-html": "^7.0.0",
    "jest-watch-typeahead": "^0.4.2",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "repo-tools": "^0.2.0",
    "size-limit": "^4.0.2",
    "ts-jest": "^25.2.1",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc",
    "build:esm": "tsc -m es6 --outDir esm",
    "clean": "del lib/* && del esm/*",
    "watch": "npm run build:cjs && npm run build:esm -- -w",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --bail --coverage",
    "test:watch": "jest --watch --verbose",
    "lint": "eslint --ext .ts,.tsx src test",
    "format": "prettier \"**/*.{js,jsx,json,yml,yaml,css,less,scss,ts,tsx,md,graphql,mdx}\" --write",
    "validate": "npm run lint && npm run test:coverage && npm run size",
    "size": "npm run build && size-limit",
    "prepublishOnly": "npm run validate"
  },
  "prettier": {
    "jsxBracketSameLine": false,
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "size-limit": [
    {
      "path": "./esm/index.js",
      "limit": "4 kB"
    },
    {
      "path": "./lib/index.js",
      "limit": "4 kB"
    }
  ],
  "husky": {
    "hooks": {
      "post-merge": "repo-tools install-packages",
      "pre-commit": "repo-tools check-remote && npm run validate"
    }
  }
}
